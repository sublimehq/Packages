              ZEND_DEBUG_BUILD | ZEND_THREAD_SAFE
            )\b
          scope: support.constant.core.php
          captures:
            1: punctuation.separator.namespace.php
        - match: |-
            (\\)?\b(?x:
              ABDAY_[1-7] | ABMON_(?:[1-9]|1[0-2]) | ALT_DIGITS | AM_STR | ASSERT_ACTIVE | ASSERT_BAIL | ASSERT_CALLBACK |
