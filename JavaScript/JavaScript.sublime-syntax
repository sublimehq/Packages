  object-property:
    - match: |-
        (?x)(?=
          {{identifier}}
          \s* = \s*
          {{either_func_lookahead}}
        )
      set:
        - function-initializer
        - function-name-meta
        - object-property-base

    - include: support-property

    - match: '(?={{identifier}}\s*\()'
      set: call-method-name

    - include: object-property-base
    - include: else-pop

