<plist version="1.0">
<dict>
	<key>scope</key>
	<string>source.python</string>
	<key>settings</key>
	<dict>
		<key>cancelCompletion</key>
		<string>(?x)
		# Prevent completions when typing keywords
		\b(and|or|pass|return)$
		| # Prevent completions for class names
		^\s* class \s+ [a-zA-Z_0-9]+ $
		</string>
	</dict>
</dict>
</plist>
